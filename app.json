{
  "expo": {
    "name": "NextUp",
    "slug": "nextup",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C2C2E"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.emeraldnomad.nextup",
      "userInterfaceStyle": "automatic",
      "buildNumber": "7",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "User's location is used to only provide courts and hoop sessions that are near the user."
      }
    },
    "android": {
      "package": "com.emeraldnomad.nextup",
      "versionCode": 7
    }
  }
}
